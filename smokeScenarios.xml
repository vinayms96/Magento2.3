<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <listeners>
        <listener class-name="com.magento.listener.Listener"></listener>
    </listeners>
    <test thread-count="5" name="Test">
        <classes>
            <class name="com.magento.browser_setup.BrowserSetup"/>
            <class name="com.magento.AppTest"/>
            <class name="com.magento.UserAccounts">
                <methods>
                    <include name="setLoggerExtent"/>
                    <exclude name="accountCreate"/>
                    <include name="accountLogin"/>
                </methods>
            </class>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->
